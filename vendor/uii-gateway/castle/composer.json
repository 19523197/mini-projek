{
    "name": "uii-gateway/castle",
    "description": "Castle, utilities for Lumen",
    "keywords": [
        "uii-gateway",
        "lumen"
    ],
    "homepage": "https://gitlab-cloud.uii.ac.id/uii-gateway/backend/castle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nabil Muhammad Firdaus",
            "email": "211232629@uii.ac.id"
        },
        {
            "name": "Bamasatya Hendraprasta",
            "email": "bamasatyaproject@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "awobaz/compoships": "^2.1",
        "bensampo/laravel-enum": "^4.2",
        "illuminate/notifications": "^8",
        "laravel/lumen-framework": "^8",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1",
        "staudenmeir/eloquent-has-many-deep": "^1.14"
    },
    "require-dev": {
        "laravel/lumen": "^8",
        "mateusjunges/laravel-kafka": "^1.6",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.0|^9.0",
        "symfony/var-dumper": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "UIIGateway\\Castle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UIIGateway\\Castle\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
